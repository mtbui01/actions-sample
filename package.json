{
  "name": "setup-ant",
  "version": "1.0.0",
  "private": true,
  "description": "setup ant",
  "main": "lib/setup-ant.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/md-actions/setup-ant.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "mtbui01",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.1.2",
    "@actions/github": "1.1.0",
    "@actions/io": "1.0.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@typescript-eslint/parser": "4.10.0",
    "@vercel/ncc": "0.25.1",
    "eslint": "7.15.0",
    "eslint-plugin-github": "4.1.1",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "js-yaml": "3.14.1",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  }
}
